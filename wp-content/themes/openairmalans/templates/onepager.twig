{# Load each section by template #}
{% extends "base.twig" %}

{% block heading %}

	{% include 'partials/front.twig' %}

{% endblock %}

{% block content %}

	{# Loop every page as section #}
	{% for post in posts %}

		{% include ['section/section-'~post.template~'.twig', 'section/section-default.twig'] %}

	{% endfor %}

{% endblock %}
